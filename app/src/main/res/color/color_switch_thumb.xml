<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled status of thumb -->
    <item android:state_enabled="false"
        android:color="@color/switch_thumb_disabled" />
    <!-- Toggle off status of thumb -->
    <item android:state_checked="false"
        android:color="@color/switch_thumb_off" />
    <!-- Enabled or toggle on status of thumb -->
    <item android:color="@color/switch_state_on" />
</selector>